{
  "identifier": "net.nurik.roman.sketch.artboardtricks",
  "authorEmail": "roman@nurik.net",
  "author": "Roman Nurik",
  "appcast": "https://api.sketchpacks.com/v1/plugins/net.nurik.roman.sketch.artboardtricks/appcast",
  "compatibleVersion": 41,
  "bundleVersion": 1,
  "name": "Artboard Tricks",
  "commands": [
    {
      "script": "command-select-artboards.js",
      "handler": "onRun",
      "shortcut": "ctrl cmd option s",
      "name": "Select Artboards Containing Selection",
      "identifier": "command-select-artboards"
    },
    {
      "script": "command-rearrange-grid.js",
      "handler": "onRun",
      "shortcut": "ctrl cmd option r",
      "name": "Rearrange Artboards Into Grid",
      "identifier": "command-rearrange-grid"
    },
    {
      "script": "command-preferences.js",
      "handler": "onRun",
      "name": "Artboard Tricks Preferences",
      "identifier": "command-preferences"
    },
    {
      "script": "command-numbers-add.js",
      "handler": "onRun",
      "shortcut": "ctrl cmd option n",
      "name": "Prefix Artboard Names With Numbers",
      "identifier": "command-numbers-add"
    },
    {
      "script": "command-numbers-remove.js",
      "handler": "onRun",
      "name": "Remove Artboard Number Prefixes",
      "identifier": "command-numbers-remove"
    }
  ],
  "menu": {
    "items": [
      "command-rearrange-grid",
      "-",
      "command-numbers-add",
      "command-numbers-remove",
      "-",
      "command-select-artboards",
      "-",
      "command-preferences"
    ],
    "title": "Artboard Tricks"
  },
  "version": "1.0.8",
  "description": "A Sketch plugin containing variety of tricks for dealing with artboards.",
  "homepage": "https://github.com/romannurik/Sketch-ArtboardTricks#readme",
  "disableCocoaScriptPreprocessor": true
}